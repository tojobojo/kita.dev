# Kita.dev UI Environment Variables
# Copy to .env.local and configure

# API URL (leave empty for same-origin proxy in dev)
VITE_API_URL=

# WebSocket URL (optional, auto-detected if empty)
VITE_WS_URL=

# API Key (if authentication is enabled)
VITE_API_KEY=

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_POLLING_INTERVAL=5000
